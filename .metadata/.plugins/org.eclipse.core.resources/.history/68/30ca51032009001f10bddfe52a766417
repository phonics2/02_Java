package Pre.propertiesEx2;
							 //비교하기 Comparable<>
public class Drama implements Comparable<Drama>{
//필드
	private String name;
	private int 출시년도;
	private int 부작;

//메서드
	//Getter
	public String getName() {
		return name;
	}
	public int get출시년도() {
		return 출시년도;
	}
	public int get부작() {
		return 부작;
	}

	//Setter
	public void setName(String name) {
		this.name = name;
	}
	public void set출시년도(int 출시년도) {
		this.출시년도 = 출시년도;
	}
	public void set부작(int 부작) {
		this.부작 = 부작;
	}

	
	//생성자 : 기본
	public Drama() {
		
	}
	//생성자 : 필수
	public Drama(String name, int 출시년도, int 부작) {
		this.name = name;
		this.출시년도 = 출시년도;
		this.부작 = 부작;
	}
	@Override
	public String toString() {
		return "name=" + name + ", 출시년도=" + 출시년도 + ", 부작=" + 부작;
	}
	
	@Override
		public int compareTo(Drama o) {
			return 0;
		}
	

	
	
}
